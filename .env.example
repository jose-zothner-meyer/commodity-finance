# Environment variables for Docker deployment
# Copy this file to .env and fill in your values

# Database Configuration
DB_PASSWORD=your_secure_database_password_here

# Django Configuration  
DJANGO_SECRET_KEY=your_very_long_secret_key_here_minimum_50_characters

# API Keys (from your api_keys.yaml)
FIN_MODELING_PREP_KEY=your_fmp_api_key_here
API_NINJAS_KEY=your_api_ninjas_key_here
COMMODITYPRICEAPI_KEY=your_commodity_api_key_here

# Optional: Additional API keys
ALPHAVANTAGE_API_KEY=your_alpha_vantage_key_here

# Monitoring (Optional)
SENTRY_DSN=your_sentry_dsn_here
